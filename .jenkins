pipeline {
    agent {
        kubernetes {
            defaultContainer 'golang'
            yaml '''
kind: Pod
spec:
containers:
- name: golang
    image: golang:1.19
'''
   }
    }
    stages {
        stage('Build') {
            steps {
                sh 'go build cmd/gitea-bulk-importer/main.go'
            }
        }
    }
}